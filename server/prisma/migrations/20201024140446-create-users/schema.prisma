generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url = "***"
}

model users {
  id          String   @id @default(uuid())
  name        String
  surname     String  @unique
  email       String   @unique
  phone       String
  gender      String
  avatar      String?  
  password    String
  created_at  DateTime @default(now()) 
  updated_at  DateTime @updatedAt
}
